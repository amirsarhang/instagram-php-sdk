    "license": "MIT",
    "authors": [
        {
            "name": "Amirhossein Sarhangian",
            "email": "ah.sarhangian@gmail.com",
            "homepage": "https://github.com/amirsarhang/instagram-php-sdk/"
        }
    ],
    "support": {
        "docs": "https://amirsarhang.github.io/instagram-php-sdk/",
        "issues": "https://github.com/amirsarhang/instagram-php-sdk/issues",
        "rss": "https://github.com/amirsarhang/instagram-php-sdk/releases.atom",
        "source": "https://github.com/amirsarhang/instagram-php-sdk.git",
        "wiki": "https://github.com/amirsarhang/instagram-php-sdk/wiki"
    },
    "require": {
        "php": ">=7.0",
        "facebook/graph-sdk": "^5.7",
        "vlucas/phpdotenv": "^5.3"
